{
  "id": "/comc1",
  "instances": 1,
  "cpus": 2,
  "mem": 5120,
  "disk": 0,
  "labels": {
    "HAPROXY_GROUP":"external",
    "HAPROXY_0_REDIRECT_TO_HTTPS":"true",
    "HAPROXY_0_VHOST":"33.33.33.11"
  },
  "constraints": [["hostname", "CLUSTER", "33.33.33.6"]],
  "ipAddress":{
      "networkName":"dcos"
   },
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "maprtech/mapr-control-cent67:5.2.0",
      "privileged": true,
      "parameters": [
                { "key": "hostname", "value": "comc1" }
            ],
      "network": "USER",
      "portMappings": [
         { 
            "containerPort": 8080,  
            "servicePort": 10001, 
            "name": "http"
         },
         {
            "containerPort": 7220,
            "servicePort": 7220,
            "name": "cldb1" 
         },
         {
            "containerPort": 7221,
            "servicePort": 7221,
            "name": "cldb2" 
         },
         {
            "containerPort": 7222,
            "servicePort": 7222,
            "name": "cldb3" 
         },
         {
            "containerPort": 1111,
            "servicePort": 1111,
            "name": "hoststats" 
         },
         {
            "containerPort": 8030,
            "servicePort": 8030,
            "name": "rm1" 
         },
         {
            "containerPort": 8031,
            "servicePort": 8031,
            "name": "rm2" 
         },
         {
            "containerPort": 8032,
            "servicePort": 8032,
            "name": "rm3" 
         },
         {
            "containerPort": 8033,
            "servicePort": 8033,
            "name": "rm4"
         },
         {
            "containerPort": 8088,
            "servicePort": 8088,
            "name": "rm"
         },
         {
            "containerPort": 10020,
            "servicePort": 10020,
            "name": "jh1"
         },
         {
            "containerPort": 19888,
            "servicePort": 19888,
            "name": "jh2"
         },
         {
            "containerPort": 50030,
            "servicePort": 50030,
            "name": "jt"
         },
         {
            "containerPort": 9090,
            "servicePort": 9090,
            "name": "hbase"
         }
      ]
    }
  },

  "healthChecks": [
    {
      "protocol": "TCP",
      "gracePeriodSeconds": 500,
      "intervalSeconds": 15,
      "timeoutSeconds": 5,
      "maxConsecutiveFailures": 20,
      "portName": "http"
    }
  ],
  "env": {
    "MEMTOTAL":"5120000",
    "DISKLIST":"/dev/vdb",
    "CLUSTERNAME":"my.cluster.com"
  }
}

